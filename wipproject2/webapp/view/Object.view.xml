<mvc:View controllerName="com.chappota.wippoc2.wipproject2.controller.Object" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic"
xmlns:si="sap.suite.ui.commons.statusindicator"
>

    <semantic:SemanticPage id="objectpage" showNavButton="true" navButtonPress="onNavBack">


        <semantic:content>
            <Table id="wiptable" items="{path : 'wipentry>/'}" mode="MultiSelect">
                <columns>
                 <Column>
                        <Text text="{i18n>txt_chngind}"/>
                    </Column>
                    <Column>
                        <Text text="{i18n>txt_jeid}"/>
                    </Column>
                    <Column>
                        <Text text="{i18n>txt_glacc}"/>
                    </Column>
                    <Column>
                         <Text text="{i18n>txt_ref}"/>
                    </Column>
                    <Column>
                         <Text text="{i18n>txt_cust}"/>
                    </Column>
                    <Column>
                         <Text text="{i18n>txt_proj}"/>
                    </Column>
                    <Column>
                         <Text text="{i18n>txt_unbqty}"/>
                    </Column>
                    <Column>
                         <Text text="{i18n>txt_wrkitem}"/>
                    </Column>
                    <Column>
                         <Text text="{i18n>txt_wrkpckg}"/>
                    </Column>
                    <Column>
                         <Text text="{i18n>txt_amnt}"/>
                    </Column>
                    <Column>
                         <Text text="{i18n>txt_notes}"/>
                    </Column>
                </columns>
                <items>
                    <ColumnListItem>
                        <cells>
                            <Button text="Edited" type="Reject" />
                            <Text text="{wipentry>AccountingDocument}"/>
                            <Text text="{wipentry>GLAccount}"/>
                            <Text text="{wipentry>ReferenceDocument}"/>
                            <Text text="{wipentry>Customer}"/>
                            <Text text="{wipentry>Project}"/>
                            <Text text="{wipentry>Quantity}"/>
                            <Text text="{wipentry>WorkItem}"/>
                            <Text text="{wipentry>WBSElement}"/>
                            <Text text="{wipentry>AmountInGlobalCurrency}"/>
                            <Text text="{wipentry>DocumentItemText}"/>
                        </cells>

                    </ColumnListItem>
                </items>
            </Table>

        </semantic:content>


    </semantic:SemanticPage>


</mvc:View>
