<mvc:View xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic" xmlns:si="sap.suite.ui.commons.statusindicator" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core"
    	xmlns:plugins="sap.m.plugins" controllerName="com.chappota.wippoc2.wipproject2.controller.Object">

    <semantic:SemanticPage id="objectpage" showNavButton="true" navButtonPress="onNavBack">


        <semantic:content>
            

            <FlexBox width="100%" id="box0" direction="Column">
                <items>
                    <Toolbar width="100%" id="toolbar1">

                        <ObjectStatus state="Error" text="Project ID: {prjlst>/ProjectID}" width="100%" id="label0" design="Bold"/>
                        <ToolbarSpacer />
                        <Label design="Bold" text="WIP Time" /><Switch type="AcceptReject" state="true" />
                        <Label design="Bold" text="WIP Expense" /><Switch type="AcceptReject" state="false" />
                        <Label design="Bold" text="WIP Unit Based" /><Switch type="AcceptReject" state="false" />
                    </Toolbar>
                </items>
            </FlexBox>
    <!-- Test -->
            <!-- <FlexBox width="100%" id="box1" wrap="Wrap" direction="Column" alignItems="Start">        
                                                                            <items>            -->

            <Toolbar width="100%" id="toolbar2" height="130px">
                <f:SimpleForm width="600px" editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="3" labelSpanM="4" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="4" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="1" columnsM="1" singleContainerFullSize="false" breakpointL="0" breakpointM="0" breakpointXL="0">
                    <f:content>
                        <Label text="Project Name"/><ObjectStatus state="Information" text="{prjlst>/ProjectName}" width="200px"/>
                        <Label text="Customer Name"/><ObjectStatus state="Information" text="{prjlst>/CustomerName}" width="200px"/>
                        <Label text="Project Controller"/><ObjectStatus state="Information" text="{prjlst>/ProjControllerName}" width="200px"/>
                    </f:content>
                </f:SimpleForm>

                <ToolbarSpacer/>
                <f:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="3" labelSpanM="4" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="4" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="1" columnsM="1" singleContainerFullSize="false" breakpointL="0" breakpointM="0" breakpointXL="0">
                    <f:content>
                        <Label text="Project Manager"/><ObjectStatus state="Information" text="{prjlst>/ProjManagerName}"/>
                        <Label text="Project Partner"/><ObjectStatus state="Information" text="{prjlst>/ProjPartnerName}"/>
                    </f:content>
                </f:SimpleForm>
            </Toolbar>

            <IconTabBar id="idIconTabBar" select=".onFilterSelect" class="sapUiResponsiveContentPadding">
                <items>

                    <IconTabFilter iconColor="Positive" count="{count1>/}" text="WIP Time" key="Ok" icon="sap-icon://time-account">
                        <content>
                            <Table id="wiptable" items="{path : 'wipentry>/'}" mode="MultiSelect" selectionChange="_wiptableselchange" growing="true" growingScrollToLoad="false"
                            growingThreshold="100">
                                <headerToolbar>
                                    <Toolbar>
                                        <content>
                                            <!-- <Title text="Time Entries" level="H2"/> -->
                                            <ObjectStatus state="Success" text="Orignal:{orignal>/}"/>
                                            <ObjectStatus state="Error" text="Changed:{changedcount>/}"/>
                                            <ObjectStatus state="Information" text="New: {newcount>/}"/>
                                            <ToolbarSpacer/>
                                            <Input id="suggestionhelpbox" placeholder="Search by typing any of the following fields.." visible="true" width="400px"
								showSuggestion="true" showTableSuggestionValueHelp="false" suggestionRows="{wipentry>/}"
								suggestionItemSelected="onSuggestionItemSelected">
								<suggestionColumns>
                                 <Column visible="false">
                                  <Text text=""/>
                                    </Column>
                                    <Column visible="false">
                                        <Text text="{i18n>txt_chngind}"/>
                                    </Column>
									<Column >
										<Label text="{i18n>txt_jeid}"/>
									</Column>
									<Column>
										<Label text="{i18n>txt_ref}"/>
									</Column>
									<Column>
										<Label text="{i18n>txt_wrkpckg}"/>
									</Column>
									<Column>
										<Label text="{i18n>txt_amnt}"/>
									</Column>
									<Column>
										<Label text="{i18n>txt_notes}"/>
									</Column>
									
								</suggestionColumns>
								<suggestionRows>
									<ColumnListItem>
<ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" icon="{= ${wipentry>StatusIcon} === undefined ? 'sap-icon://accept' : ${wipentry>StatusIcon}}"/>
<ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" text="{= ${wipentry>Status} === undefined ? 'Original' : ${wipentry>Status}}"/>

<ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" text="{wipentry>AccountingDocument}"/>

<ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" text="{wipentry>ReferenceDocument}"/>


<ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" text="{wipentry>WBSElement}"/>
<ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" text="{wipentry>AmountInGlobalCurrency}"/>
<ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" text="{wipentry>DocumentItemText}"/>

									</ColumnListItem>
								</suggestionRows>
							</Input>
                                            <Input width="auto" placeholder="Accouting Document..." submit="_searchaccdoc" id="_search" visible="false"/>
                                            <Button icon="sap-icon://refresh" press="_refreshWipTable"/>
                                            <Button text="New" width="auto" press="_newjecreation"/>
                                            <Button text="Edit" width="auto" press="_updatejeRecord"/>
                                            <Button text="Delete" width="auto" press="_deleteRecord"/>
                                            <Button text="Transfer" width="auto" visible="false"/>
                                            <Button text="Combine" width="auto"/>
                                            
                                            <Button
                                            text="Post/Submit" width="auto" press="_finalizeRecord"/>
                                            <!-- <CheckBox text="Fixed Layout" selected="true" select="onCheckBoxSelect"/> -->
                                            <Button text="Ready To Bill" width="auto"/>
                                            <Button icon="sap-icon://action-settings" press="onPersoButtonPressed">
                                                <!-- <layoutData>
                                                    <OverflowToolbarLayoutData priority="NeverOverflow"/>
                                                </layoutData> -->
                                            </Button>
                                        </content>
                                    </Toolbar>
                                </headerToolbar>
                                	<dependents>
                                        <plugins:ColumnResizer/>
                                    </dependents>
                               <columns>
                                    <Column width="20px">
                                        <Text text=""/>
                                    </Column>
                                    <Column>
                                        <Text text="{i18n>txt_chngind}"/>
                                    </Column>

                                    <Column width="100px">
                                        <Text text="{i18n>txt_jeid}"/>
                                    </Column>
                                    <Column width="100px">
                                        <Text text="{i18n>txt_glacc}"/>
                                    </Column>
                                    <Column >
                                        <Text text="{i18n>txt_ref}"/>
                                    </Column>
                                    <Column>
                                        <Text text="{i18n>txt_cust}"/>
                                    </Column>
                                    <Column >
                                        <Text text="{i18n>txt_proj}"/>
                                    </Column>
                                    <Column >
                                        <Text text="{i18n>txt_unbqty}"/>
                                    </Column>
                                    <Column >
                                        <Text text="{i18n>txt_wrkitem}"/>
                                    </Column>
                                    <Column >
                                        <Text text="{i18n>txt_wrkpckg}"/>
                                    </Column>
                                    <Column >
                                        <Text text="{i18n>txt_amnt}"/>
                                    </Column>
                                      <Column>
                                        <Text text="{i18n>txt_acttype}"/>
                                    </Column>
                                   
                                     <Column width="100px">
                                        <Text text="Personnel Number"/>
                                    </Column>
                                     <Column >
                                        <Text text="Service Rendered Date"/>
                                    </Column>
                                     <Column >
                                        <Text text="{i18n>txt_notes}"/>
                                    </Column>                                                                        
                                </columns>
                                <items>
                                    <ColumnListItem>
                                        <cells>
                                            <ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" icon="{= ${wipentry>StatusIcon} === undefined ? 'sap-icon://accept' : ${wipentry>StatusIcon}}"/>
                                            <ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" text="{= ${wipentry>Status} === undefined ? 'Original' : ${wipentry>Status}}"/>
                                            <ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" text="{wipentry>AccountingDocument}"/>
                                            <ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" text="{wipentry>GLAccount}"/>
                                            <ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" text="{wipentry>ReferenceDocument}"/>
                                            <ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" text="{wipentry>Customer}"/>
                                            <ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" text="{wipentry>Project}"/>
                                            <ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" text="{wipentry>Quantity}"/>
                                            <ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" text="{wipentry>WorkItem}"/>
                                            <ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" text="{wipentry>WBSElement}"/>
                                            <ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" text="{wipentry>AmountInGlobalCurrency}"/>
                                            <ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" text="{wipentry>OriginCostCtrActivityType}"/>                                          
                                            <ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" text="{wipentry>PersonnelNumber}"/>
                                            <ObjectStatus state="{= ${wipentry>StatusObject} === undefined ? 'Success' : ${wipentry>StatusObject}}" text="{path : 'wipentry>ServicesRenderedDate',formatter:'.formatter.dateTime'}"/>
                                              <Link  text="{wipentry>DocumentItemText}" width="80px" press="_notespopover" wrapping="false"/>
                                            <!-- <Button  icon="sap-icon://list" press="_notespopover" type="Transparent"/> -->
                                        </cells>

                                    </ColumnListItem>
                                </items>
                            </Table>
                        </content>
                    </IconTabFilter>
                    <!--                                   <IconTabFilter icon="sap-icon://inventory" iconColor="Negative" count="{/ProductCollectionStats/Counts/Weight/Overweight}" text="Overweight" key="Overweight"/> -->
                    <IconTabFilter icon="sap-icon://expense-report" iconColor="Critical" text="Ready To Bill" key="wipexpense" visible="true">
                        <content>
                          <Text text="Ready To Bill" />
                        </content>

                    </IconTabFilter>
                    <!-- <IconTabFilter icon="sap-icon://tools-opportunity" iconColor="Negative" text="WIP Unit Based" key="wipunitbased" visible="true">
                        <content>
                            <Text text="WIP Unit Based" />
                        </content>

                    </IconTabFilter> -->
                </items>

            </IconTabBar>

        </semantic:content>


    </semantic:SemanticPage>


</mvc:View>
