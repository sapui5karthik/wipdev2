            <IconTabBar id="idIconTabBar" select=".onFilterSelect" class="sapUiResponsiveContentPadding">
                <items>

                    <IconTabFilter iconColor="Positive" count="45" text="WIP Time" key="Ok" icon="sap-icon://time-account"/>
